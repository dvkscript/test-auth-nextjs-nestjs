{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migrate": "npx ts-node dist/database/migrate.js",
    "postinstall": "patch-package",
    "migration:create": "ts-node src/database/migrate create --name",
    "migration:undo": "ts-node src/database/rollback",
    "migration:up": "ts-node src/database/migrate up",
    "migration:up:tests": "NODE_ENV=test ts-node src/database/migrate up",
    "migration:down": "ts-node src/database/migrate down",
    "migration:down:tests": "NODE_ENV=test ts-node src/database/migrate down",
    "migration:executed": "ts-node src/database/migrate executed",
    "migration:executed:tests": "NODE_ENV=test ts-node src/database/migrate executed",
    "migration:pending": "ts-node src/database/migrate pending",
    "migration:pending:tests": "NODE_ENV=test ts-node src/database/migrate pending",
    "seed:up": "ts-node src/database/seed"
  },
  "dependencies": {
    "@apollo/server": "^4.11.3",
    "@nestjs/apollo": "^13.0.2",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.1",
    "@nestjs/cqrs": "^11.0.2",
    "@nestjs/event-emitter": "^3.0.0",
    "@nestjs/graphql": "^13.0.2",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.0.3",
    "@slonik/migrator": "^0.12.0",
    "ajv": "^8.17.1",
    "bcryptjs": "^3.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "env-var": "^7.5.0",
    "graphql": "^16.10.0",
    "nanoid": "^3.3.8",
    "nestjs-console": "^10.0.0",
    "nestjs-request-context": "^3.0.0",
    "nestjs-slonik": "^10.0.2",
    "oxide.ts": "^1.1.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "patch-package": "^8.0.0",
    "pg": "^8.13.3",
    "postinstall-postinstall": "^2.1.0",
    "reflect-metadata": "^0.2.2",
    "run-script-webpack-plugin": "^0.2.1",
    "rxjs": "^7.8.1",
    "slonik": "^46.4.0",
    "webpack": "^5.97.1",
    "webpack-node-externals": "^3.0.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.4",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
